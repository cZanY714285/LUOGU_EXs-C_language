#include <stdio.h>
#include <limits.h>
int main(){
    int i = 0;
    int n = 0;
    int min = INT_MAX;
    int a[1000];
    scanf("%d",&n);
    for (i = 0;i < n;i++) {
        scanf("%d",&a[i]);
        if (min > a[i]) {
            min = a[i];
        }
    }
    printf("%d",min);
    return 0;
}
