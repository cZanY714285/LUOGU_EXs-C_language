#include <stdio.h>
#include <math.h>
// int a[10000] = {0};
// scanf("%d",&a[i]);
int main() {
    int k = 0;
    double n = 1;
    double sum = 0;
    scanf("%d",&k);
    while (sum <= k) {
        sum += 1.0/n;
        //printf("sum = %f\n",sum);
        //printf("1 / %f =%f\n",n,1 /n);
        //printf("n = %f\n",n);
        n++;
    }
    printf("%.0f",n-1);
    return 0;
}
