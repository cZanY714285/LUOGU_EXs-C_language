#include <stdio.h>
#include <math.h>

int main(){
    int n;
    scanf("%d",&n);
    switch(n) {
        case 1:
            case 3:
        case 5:
        printf("NO");
        break;
        case 2:
            case 4:
        case 6:
        case 7:
        printf("YES");
    }
    return 0;

}
