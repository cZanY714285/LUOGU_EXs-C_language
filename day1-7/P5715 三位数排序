#include <stdio.h>
#include <limits.h>
int main(){
    int a,b,c,temp = 0;
    scanf("%d %d %d",&a,&b,&c);
    if (a > b) {
        temp = b;
        b = a;
        a = temp;
    }if (b > c) {
        temp = c;
        c = b;
        b = temp;
    }if (a > b) {
        temp = b;
        b = a;
        a = temp;
    }printf("%d %d %d",a,b,c);
    return 0;
}
