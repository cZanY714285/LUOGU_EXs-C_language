#include <stdio.h>
#include <math.h>

int main(){
    int x,y,z;
    scanf("%d %d %d",&x,&y,&z);
    if (x < 60 && y >= 60 && z >= 60) {
        printf("1");
    }else if (x >= 60 && y < 60 && z >= 60) {
        printf("1");
    }else if (x >= 60 && y >= 60 && z < 60) {
        printf("1");
    }else {
        printf("0");
    }

    return 0;

}
