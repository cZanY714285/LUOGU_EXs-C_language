#include <stdio.h>
#include <math.h>

int main() {
    long long  a,b = 0;
    scanf("%lld %lld",&a,&b);

    if (a == 1) {
        printf("1");
    }else if (pow(a,b) > pow(10,9)) {
        printf("-1");
    }else {
      //  printf("a = %lld\n",a);
       // printf("b = %lld\n",b);
        printf("%lld",(long long)pow(a,b));
    }
    return 0;
}
